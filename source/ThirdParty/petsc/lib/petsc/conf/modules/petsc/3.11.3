#%Module

proc ModulesHelp { } {
    puts stderr "This module sets the path and environment variables for petsc-3.11.3"
    puts stderr "     see http://www.mcs.anl.gov/petsc/ for more information      "
    puts stderr ""
}
module-whatis "PETSc - Portable, Extensible Toolkit for Scientific Computation"

set petsc_dir   "/home/biswajit/project/sFVM/source/ThirdParty/petsc"
set petsc_arch  ""

setenv PETSC_ARCH "$petsc_arch"
setenv PETSC_DIR "$petsc_dir"
prepend-path PATH "/home/biswajit/project/sFVM/source/ThirdParty/petsc/bin"
